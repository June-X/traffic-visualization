<?xml version="1.0" encoding="utf-8"?>

<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:fb="http://ns.adobe.com/flashbuilder/2009" >
	
	<fx:Metadata>
		<![CDATA[ 
		[HostComponent("spark.components.SkinnableContainer")]
		]]>
	</fx:Metadata> 
	
	
	<s:states>
		<s:State name="normal" />
		<s:State name="disabled" />
		<s:State name="hovered" />
	</s:states>
	
	<s:Rect left="0" right="0" top="0" bottom="0" radiusX="5" radiusY="5">
		<s:stroke>
			<s:LinearGradientStroke weight="0.5"/>
		</s:stroke>
		<s:fill>
			<s:LinearGradient rotation="90">
				<s:entries>
					<s:GradientEntry id="rf1" color="0xA2B700" ratio="0.00" alpha.normal="0.8" alpha.hovered="1.0"/>
					<!--s:GradientEntry id="rf2" color="0x9966FF" ratio="1.0" alpha.normal="0.5" alpha.hovered="1.0"/-->
					<!--s:GradientEntry id="rf3" color="0xCCCCCC" ratio="0.5" alpha.normal=".8" alpha.hovered="1.0"/-->
				</s:entries>
			</s:LinearGradient>    
		</s:fill>
	</s:Rect>
	
	<s:Group id="contentGroup" left="0" right="0" top="0" bottom="0" minWidth="0" minHeight="0">
		<s:layout>
			<s:BasicLayout/>
		</s:layout>
	</s:Group>
	
</s:Skin>